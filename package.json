{
  "name": "claude-sdk",
  "version": "0.1.13",
  "description": "Claude SDK: manage Solana wallets with an AI agent",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "assets"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --sourcemap --clean --dts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18"
  }
}
